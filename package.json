{
  "name": "api-toggler",
  "version": "0.5.0",
  "description": "Alias your app API with other API",
  "license": "MIT",
  "repository": "VovanR/api-toggler",
  "homepage": "https://github.com/VovanR/api-toggler#readme",
  "bugs": "https://github.com/VovanR/api-toggler/issues",
  "author": "Vladimir Rodkin <mail@vovanr.com> (https://github.com/VovanR)",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "xo && nyc ava",
    "tdd": "ava -w",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "api",
    "alias",
    "convert"
  ],
  "devDependencies": {
    "ava": "^3.15.0",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^5.1.3",
    "mversion": "^2.0.1",
    "nyc": "^15.1.0",
    "xo": "^0.38.2"
  },
  "xo": {
    "esnext": false
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
