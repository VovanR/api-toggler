  "keywords": [
    "api",
    "alias",
    "convert"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.14.0",
    "conventional-changelog-cli": "^1.1.1",
    "cz-conventional-changelog": "^1.1.6",
    "coveralls": "^2.11.9",
    "husky": "^0.11.4",
    "mversion": "^1.10.1",
    "nyc": "^6.4.0",
    "xo": "^0.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
