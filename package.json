{
  "name": "api-toggler",
  "version": "0.2.0",
  "description": "Alias your app API with other API",
  "license": "MIT",
  "repository": "VovanR/api-toggler",
  "author": "Vladimir Rodkin <mail@vovanr.com> (https://github.com/VovanR)",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "prepush": "npm test",
    "test": "xo --no-esnext && nyc ava",
    "tdd": "ava -w",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major"
  },
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "keywords": [
    "api",
    "alias",
    "convert"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.25.0",
    "conventional-changelog-cli": "^1.3.9",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "mversion": "^1.10.1",
    "nyc": "^11.4.1",
    "xo": "^0.18.2"
  },
  "bugs": {
    "url": "https://github.com/VovanR/api-toggler/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
