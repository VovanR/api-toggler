{
  "name": "api-toggler",
  "version": "0.1.1",
  "description": "Alias your app API with other API",
  "license": "MIT",
  "repository": "VovanR/api-toggler",
  "author": {
    "name": "Vladimir Rodkin",
    "email": "mail@vovanr.com",
    "url": "https://github.com/VovanR"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "prepush": "npm test",
    "test": "xo && nyc ava",
